<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="6_tenants" parallel="false" verbose="2">
    <test name="tenantsChromeTest">
        <parameter name="browser" value="CHROME_LINUX_64"/>
        <parameter name="environment" value="dev"/>
        <groups>
            <run>
                <include name="init"></include>
                <include name="login"></include>
                <include name="tenantCreate"></include>
                <include name="tenantOverviewRead"></include>
                <include name="tenantReadStyles"></include>
                <include name="tenantReadIntoRole"></include>
                <include name="tenantReadIntoUser"></include>
                <include name="tenantDelete"></include>
                <include name="tenantDeactive"></include>
                <include name="tenantEdit"></include>
                <include name="tenantEditError"></include>
                <include name="tenantChangeStyle"></include>
                <include name="tenantResetStyles"></include>
            </run>
        </groups>
        <classes>
            <class name="com.janeirodigital.xform.webdriver.scripts.Initializer"/>
            <class name="com.janeirodigital.xform.webdriver.scripts.AccessAndSecurity"/>
            <class name="com.janeirodigital.xform.webdriver.scripts.Tenant"/>
        </classes>
    </test>
</suite>