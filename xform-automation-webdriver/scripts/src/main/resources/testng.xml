<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestSuite" parallel="false" verbose="2">
    <listeners>
        <listener class-name="com.janeirodigital.xform.webdriver.listeners.TestListener"/>
    </listeners>

    <test name="ChromeTest">
        <parameter name="browser" value="CHROME_LINUX_64"/>
        <parameter name="environment" value="dev"/>
        <groups>
            <run>
                <include name="parent"></include>
                <include name="Script_2_GeneralUserInterface"></include>
            </run>
        </groups>
        <classes>
            <class name="com.janeirodigital.xform.webdriver.scripts.Scripts" />
            <class name="com.janeirodigital.xform.webdriver.scripts.Script_2_GeneralUserInterface" />
        </classes>
    </test>


</suite>